"    \n"
"    project (*)\n"
"    browser (**)\n"
"    batch-search (*)\n"
"    interactive-search\n"
"    source-assistant (**)\n"
"    debug-data\n"
"    debug-stack\n"
"    debug-io\n"
"    debug-exceptions\n"
"    debug-breakpoints (**)\n"
"    debug-probe (**)\n"
"    debug-watch (**)\n"
"    debug-modules (**)\n"
"    python-shell\n"
"    messages (*)\n"
"    help\n"
