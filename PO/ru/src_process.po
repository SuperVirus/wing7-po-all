msgstr "(%s) на хосте %s (%s) процесс id %s"

#: src/process/abstract.py:599
msgid "No other users at this time."
msgstr "Других пользователей на данный момент нет."

#: src/process/abstract.py:637
msgid "Current OS '%s' is not supported"
msgstr "Текущая ОС '%s' не поддерживается"

#: src/process/abstract.py:641
msgid "License OS '%s' does not match current OS '%s'"
msgstr "Лицензия ОС '%s' не соответствует текущей ОС '%s'"

#: src/process/abstract.py:706
msgid "expired"
msgstr "истекший"

#: src/process/abstract.py:711
msgid "Missing a required line %s"
msgstr "Отсутствует требуемая строка %s"

#: src/process/abstract.py:763
msgid "Invalid license activation"
msgstr "Недействительная активация лицензии"

#: src/process/abstract.py:768
msgid "unlimited"
msgstr "неограниченный"

#: src/process/abstract.py:770 src/process/abstract.py:774
